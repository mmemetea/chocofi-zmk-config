#include <behaviors.dtsi>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/mouse.h>
#include <dt-bindings/zmk/outputs.h>
#include <dt-bindings/zmk/ext_power.h>

#define UNDO LG(Z)
#define CUT LG(X)
#define COPY LG(C)
#define PASTE LG(V)
#define REDO LS(LG(Z))

/ {
	chosen {
		zmk,physical-layout = &foostan_corne_5col_layout;
	};
};


/ {
	macros {
		/* Macro to select Bluetooth device 0 */
		u_macro_u_bt_sel_0: u_macro_u_bt_sel_0 {
			compatible = "zmk,behavior-macro";
			#binding-cells = <0>;
			wait-ms = <0>;
			bindings = <&bt BT_SEL 0 &bt BT_CLR>;
		};

		/* Macro to select Bluetooth device 1 */
		u_macro_u_bt_sel_1: u_macro_u_bt_sel_1 {
			compatible = "zmk,behavior-macro";
			#binding-cells = <0>;
			wait-ms = <0>;
			bindings = <&bt BT_SEL 1 &bt BT_CLR>;
		};

		/* Macro to select Bluetooth device 2 */
		u_macro_u_bt_sel_2: u_macro_u_bt_sel_2 {
			compatible = "zmk,behavior-macro";
			#binding-cells = <0>;
			wait-ms = <0>;
			bindings = <&bt BT_SEL 2 &bt BT_CLR>;
		};

		/* Macro to select Bluetooth device 3 */
		u_macro_u_bt_sel_3: u_macro_u_bt_sel_3 {
			compatible = "zmk,behavior-macro";
			#binding-cells = <0>;
			wait-ms = <0>;
			bindings = <&bt BT_SEL 3 &bt BT_CLR>;
		};

		/* Macro to select Bluetooth device 4 */
		u_macro_u_bt_sel_4: u_macro_u_bt_sel_4 {
			compatible = "zmk,behavior-macro";
			#binding-cells = <0>;
			wait-ms = <0>;
			bindings = <&bt BT_SEL 4 &bt BT_CLR>;
		};
	};
};


/ {
	behaviors {
		u_mt: u_mt {
			compatible = "zmk,behavior-hold-tap";
			#binding-cells = <2>;
			tapping-term-ms = <200>;
			quick-tap-ms = <0>;
			flavor = "balanced";
			bindings = <&kp>, <&kp>;
		};

		u_lt: u_lt {
			compatible = "zmk,behavior-hold-tap";
			#binding-cells = <2>;
			tapping-term-ms = <200>;
			quick-tap-ms = <0>;
			flavor = "balanced";
			bindings = <&mo>, <&kp>;
		};

		/* Behavior to toggle Bluetooth device 0 */
		u_bt_sel_0: u_bt_sel_0 {
			compatible = "zmk,behavior-mod-morph";
			#binding-cells = <0>;
			bindings = <&bt BT_SEL 0>, <&u_macro_u_bt_sel_0>;
			mods = <(MOD_LSFT|MOD_RSFT)>;
		};

		/* Behavior to toggle Bluetooth device 1 */
		u_bt_sel_1: u_bt_sel_1 {
			compatible = "zmk,behavior-mod-morph";
			#binding-cells = <0>;
			bindings = <&bt BT_SEL 1>, <&u_macro_u_bt_sel_1>;
			mods = <(MOD_LSFT|MOD_RSFT)>;
		};

		/* Behavior to toggle Bluetooth device 2 */
		u_bt_sel_2: u_bt_sel_2 {
			compatible = "zmk,behavior-mod-morph";
			#binding-cells = <0>;
			bindings = <&bt BT_SEL 2>, <&u_macro_u_bt_sel_2>;
			mods = <(MOD_LSFT|MOD_RSFT)>;
		};

		/* Behavior to toggle Bluetooth device 3 */
		u_bt_sel_3: u_bt_sel_3 {
			compatible = "zmk,behavior-mod-morph";
			#binding-cells = <0>;
			bindings = <&bt BT_SEL 3>, <&u_macro_u_bt_sel_3>;
			mods = <(MOD_LSFT|MOD_RSFT)>;
		};

		/* Behavior to toggle Bluetooth device 4 */
		u_bt_sel_4: u_bt_sel_4 {
			compatible = "zmk,behavior-mod-morph";
			#binding-cells = <0>;
			bindings = <&bt BT_SEL 4>, <&u_macro_u_bt_sel_4>;
			mods = <(MOD_LSFT|MOD_RSFT)>;
		};

		/* Behavior to toggle output */
		u_out_tog: u_out_tog {
			compatible = "zmk,behavior-mod-morph";
			#binding-cells = <0>;
			bindings = <&out OUT_TOG>, <&out OUT_USB>;
			mods = <(MOD_LSFT|MOD_RSFT)>;
		};

		/* Behavior to toggle Caps Lock word */
		u_caps_word: u_caps_word {
			compatible = "zmk,behavior-mod-morph";
			#binding-cells = <0>;
			bindings = <&caps_word>, <&kp CAPS>;
			mods = <(MOD_LSFT|MOD_RSFT)>;
		};

		/* Behavior to tap-dance to Base layer */
		u_to_U_BASE: u_to_U_BASE {
			compatible = "zmk,behavior-tap-dance";
			#binding-cells = <0>;
			tapping-term-ms = <200>;
			bindings = <&trans>, <&to 0>;
		};

		/* Behavior to tap-dance to Extra layer */
		u_to_U_EXTRA: u_to_U_EXTRA {
			compatible = "zmk,behavior-tap-dance";
			#binding-cells = <0>;
			tapping-term-ms = <200>;
			bindings = <&trans>, <&to 1>;
		};

		/* Behavior to tap-dance to Tap layer */
		u_to_U_TAP: u_to_U_TAP {
			compatible = "zmk,behavior-tap-dance";
			#binding-cells = <0>;
			tapping-term-ms = <200>;
			bindings = <&trans>, <&to 2>;
		};

		/* Behavior to tap-dance to Button layer */
		u_to_U_BUTTON: u_to_U_BUTTON {
			compatible = "zmk,behavior-tap-dance";
			#binding-cells = <0>;
			tapping-term-ms = <200>;
			bindings = <&trans>, <&to 3>;
		};

		/* Behavior to tap-dance to Navigation layer */
		u_to_U_NAV: u_to_U_NAV {
			compatible = "zmk,behavior-tap-dance";
			#binding-cells = <0>;
			tapping-term-ms = <200>;
			bindings = <&trans>, <&to 4>;
		};

		/* Behavior to tap-dance to Mouse layer */
		u_to_U_MOUSE: u_to_U_MOUSE {
			compatible = "zmk,behavior-tap-dance";
			#binding-cells = <0>;
			tapping-term-ms = <200>;
			bindings = <&trans>, <&to 5>;
		};

		/* Behavior to tap-dance to Media layer */
		u_to_U_MEDIA: u_to_U_MEDIA {
			compatible = "zmk,behavior-tap-dance";
			#binding-cells = <0>;
			tapping-term-ms = <200>;
			bindings = <&trans>, <&to 6>;
		};

		/* Behavior to tap-dance to Number layer */
		u_to_U_NUM: u_to_U_NUM {
			compatible = "zmk,behavior-tap-dance";
			#binding-cells = <0>;
			tapping-term-ms = <200>;
			bindings = <&trans>, <&to 7>;
		};

		/* Behavior to tap-dance to Symbol layer */
		u_to_U_SYM: u_to_U_SYM {
			compatible = "zmk,behavior-tap-dance";
			#binding-cells = <0>;
			tapping-term-ms = <200>;
			bindings = <&trans>, <&to 8>;
		};

		/* Behavior to tap-dance to Function layer */
		u_to_U_FUN: u_to_U_FUN {
			compatible = "zmk,behavior-tap-dance";
			#binding-cells = <0>;
			tapping-term-ms = <200>;
			bindings = <&trans>, <&to 9>;
		};
	};
};

/ {
	keymap {
		compatible = "zmk,keymap";

	/* Base layer */
	BASE {
		display-name = "Base";
		bindings = <
			&kp Q			&kp W			&kp F			&kp P			&kp B			&kp J			&kp L			&kp U			&kp Y			&kp SQT
			&u_mt LGUI A		&u_mt LALT R		&u_mt LCTRL S		&u_mt LSHFT T		&kp G			&kp M			&u_mt LSHFT N		&u_mt LCTRL E		&u_mt LALT I		&u_mt LGUI O
			&u_lt 3 Z		&u_mt RALT X		&kp C			&kp D			&kp V			&kp K			&kp H			&kp COMMA		&u_mt RALT DOT		&u_lt 3 SLASH
									&u_lt 6 ESC		&u_lt 4 SPACE		&u_lt 5 TAB		&u_lt 8 RET		&u_lt 7 BSPC		&u_lt 9 DEL
			>;
		};

	/* Extra layer */
	EXTRA {
		display-name = "Extra";
		bindings = <
			&kp Q			&kp W			&kp E			&kp R			&kp T			&kp Y			&kp U			&kp I			&kp O			&kp P
			&u_mt LGUI A		&u_mt LALT S		&u_mt LCTRL D		&u_mt LSHFT F		&kp G			&kp H			&u_mt LSHFT J		&u_mt LCTRL K		&u_mt LALT L		&u_mt LGUI SQT
			&u_lt 3 Z		&u_mt RALT X		&kp C			&kp V			&kp B			&kp N			&kp M			&kp COMMA		&u_mt RALT DOT		&u_lt 3 SLASH
									&u_lt 6	ESC		&u_lt 4 SPACE		&u_lt 5 TAB		&u_lt 8 RET		&u_lt 7 BSPC		&u_lt 9 DEL
			>;
		};

	/* Tap layer */
	TAP {
		display-name = "Tap";
		bindings = <
			&kp Q			&kp W			&kp F			&kp P			&kp B			&kp J			&kp L			&kp U			&kp Y			&kp SQT
			&kp A			&kp R			&kp S			&kp T			&kp G			&kp M			&kp N			&kp E			&kp I			&kp O
			&kp Z			&kp X			&kp C			&kp D			&kp V			&kp K			&kp H			&kp COMMA		&kp DOT			&kp SLASH
									&kp ESC			&kp SPACE		&kp TAB			&kp RET			&kp BSPC		&kp DEL
			>;
		};

	/* Button layer */
	BUTTON {
		display-name = "Button";
		bindings = <
			&kp UNDO		&kp CUT		&kp COPY		&kp PASTE		&kp REDO		&kp LS(LG(Z))		&kp LG(V)		&kp LG(C)		&kp LG(X)		&kp LG(Z)
			&kp LGUI		&kp LALT		&kp LCTRL		&kp LSHFT		&trans			&trans			&kp LSHFT		&kp LCTRL		&kp LALT		&kp LGUI
			&kp LG(Z)		&kp LG(X)		&kp LG(C)		&kp LG(V)		&kp LS(LG(Z))		&kp LS(LG(Z))		&kp LG(V)		&kp LG(C)		&kp LG(X)		&kp LG(Z)
									&mkp MB3		&mkp MB1		&mkp MB2		&mkp MB2		&mkp MB1		&mkp MB3
			>;
		};

	/* Navigation layer */
	NAV {
		display-name = "Nav";
		bindings = <
			&trans			&u_to_U_TAP		&u_to_U_EXTRA		&u_to_U_BASE		&trans			&kp LS(LG(Z))		&kp LG(V)		&kp LG(C)		&kp LG(X)		&kp LG(Z)
			&kp LGUI		&kp LALT		&kp LCTRL		&kp LSHFT		&trans			&u_caps_word		&kp LEFT		&kp DOWN		&kp UP			&kp RIGHT
			&trans			&kp RALT		&u_to_U_NUM		&u_to_U_NAV		&trans			&kp INS			&kp HOME		&kp PG_DN		&kp PG_UP		&kp END
									&trans			&trans			&trans			&kp RET			&kp BSPC		&kp DEL
			>;
		};

	/* Mouse layer */
	MOUSE {
		display-name = "Mouse";
		bindings = <
			&trans			&u_to_U_TAP		&u_to_U_EXTRA		&u_to_U_BASE		&trans			&kp LS(LG(Z))		&kp LG(V)		&kp LG(C)		&kp LG(X)		&kp LG(Z)
			&kp LGUI		&kp LALT		&kp LCTRL		&kp LSHFT		&trans			&trans			&kp KP_N4		&kp KP_N2		&kp KP_N8		&kp KP_N6
			&trans			&kp RALT		&u_to_U_SYM		&u_to_U_MOUSE		&trans			&trans			&trans			&trans			&trans			&trans
									&trans			&trans			&trans			&mkp MB2		&mkp MB1		&mkp MB3
			>;
		};

	/* Media layer */
	MEDIA {
		display-name = "Media";
		bindings = <
			&bootloader		&u_to_U_TAP		&u_to_U_EXTRA		&u_to_U_BASE		&trans			&trans			&trans			&trans			&trans			&trans
			&kp LGUI		&kp LALT		&kp LCTRL		&kp LSHFT		&trans			&ext_power EP_TOG	&kp C_PREV		&kp C_VOL_DN		&kp C_VOL_UP		&kp C_NEXT
			&studio_unlock		&kp RALT		&u_to_U_FUN		&u_to_U_MEDIA		&trans			&u_out_tog		&u_bt_sel_0		&u_bt_sel_1		&u_bt_sel_2		&u_bt_sel_3
															&trans			&trans			&trans			&kp C_STOP		&kp C_PP		&kp C_MUTE
			>;
		};

	/* Number layer */
	NUM {
		display-name = "Num";
		bindings = <
			&kp LBKT		&kp N7			&kp N8			&kp N9			&kp RBKT		&trans			&u_to_U_BASE		&u_to_U_EXTRA		&u_to_U_TAP		&trans
			&kp SEMI		&kp N4			&kp N5			&kp N6			&kp EQUAL		&trans			&kp LSHFT		&kp LCTRL		&kp LALT		&kp LGUI
			&kp GRAVE		&kp N1			&kp N2			&kp N3			&kp BSLH		&trans			&u_to_U_NUM		&u_to_U_NAV		&kp RALT		&trans
									&kp DOT			&kp N0			&kp MINUS		&trans			&trans			&trans
			>;
		};

	/* Symbol layer */
	SYM {
		display-name = "Sym";
		bindings = <
			&kp LBRC		&kp AMPS		&kp ASTRK		&kp LPAR		&kp RBRC		&trans			&u_to_U_BASE		&u_to_U_EXTRA		&u_to_U_TAP		&trans
			&kp COLON		&kp DLLR		&kp PRCNT		&kp CARET		&kp PLUS		&trans			&kp LSHFT		&kp LCTRL		&kp LALT		&kp LGUI
			&kp TILDE		&kp EXCL		&kp AT			&kp HASH		&kp PIPE		&trans			&u_to_U_SYM		&u_to_U_MOUSE		&kp RALT		&trans
									&kp LPAR		&kp RPAR		&kp UNDER		&trans			&trans		&trans
			>;
		};

	/* Function layer */
	FUN {
		display-name = "Fun";
		bindings = <
			&kp F12			&kp F7			&kp F8			&kp F9			&kp PSCRN		&trans			&u_to_U_BASE		&u_to_U_EXTRA		&u_to_U_TAP		&bootloader
			&kp F11			&kp F4			&kp F5			&kp F6			&kp SLCK		&trans			&kp LSHFT		&kp LCTRL		&kp LALT		&kp LGUI
			&kp F10			&kp F1			&kp F2			&kp F3			&kp PAUSE_BREAK		&trans			&u_to_U_FUN		&u_to_U_MEDIA		&kp RALT		&trans
									&kp K_APP		&kp SPACE		&kp TAB			&trans			&trans		&trans
			>;
		};
	};
};